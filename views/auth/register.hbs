      <div class="Reg">
        <div class="RegLogo">
          <img
            src="/images/img/Group%201216.svg"
          />
        </div>
        <h2>Регистрация</h2>
        <form action="/admin/register" method="POST">
        <div>
          
        <input
          placeholder="Имя"
          oninput="Name()"
          id="Name"
          class="Inp"
          required,
          name="name"
        />
        <small id="Error" class="Error"></small>      
          <input
            placeholder="Фамилия"
            oninput="surName()"
            id="surName"
            class="Inp"
            required,
            name="surname"
          />
          <small id="Error1" class="Error"></small>     
          <input
            type="email"
            placeholder="email"
            oninput="Email()"
            id="Email"
            class="Inp"
            required,
            name="email"
          />
          <small id="Error2" class="Error"></small>      
          <input
            type="tel"
            placeholder="Телефон"
            id="Tel"
            class="Inp"
            oninput="Tel()"
            required
            name="phone_number"
          />
          <small id="Error3" class="Error"></small>
          <input
            type="password"
            placeholder="Пароль"
            id="Pass"
            class="Inp"
            oninput="Password()"
            required,
            name="password"
          />
          <small id="Error4" class="Error"></small>     
          <input
            type="password"
            oninput="passwordRep()"
            placeholder="Повторить пароль"
            id="PassRep"
            class="Inp"
          />
          <small id="Error5" class="Error"></small>
        </div>
        
        <button onclick="RegEnd()">Регистрация</button>
        </form>
        <div class="RegEnd">
          <p>Уже зарегистрированы?</p>
          <a href="/admin/login">Логин</a>
        </div>
    </header>
     <script src="/js/helper.js"></script>
     <script src="/js/script.js"></script>
</html>